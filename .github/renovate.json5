{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "docker:enableMajor",
    "helpers:pinGitHubActionDigests",
    "replacements:k8s-registry-move",
    ":automergeBranch",
    ":disableRateLimiting",
    ":dependencyDashboard",
    ":semanticCommits",
    ":skipStatusChecks",
    ":timezone(America/New_York)",
    "replacements:all",
    "workarounds:all",
    "group:monorepos",
    "github>kiliantyler/kubernetes-yaml-formatter//.github/renovate/autoMerge.json5",
    "github>kiliantyler/kubernetes-yaml-formatter//.github/renovate/commitMessage.json5",
    "github>kiliantyler/kubernetes-yaml-formatter//.github/renovate/labels.json5",
    "github>kiliantyler/kubernetes-yaml-formatter//.github/renovate/semanticCommits.json5",
    "github>kiliantyler/kubernetes-yaml-formatter//.github/renovate/customManagers.json5"
  ],
  "dependencyDashboardTitle": "Renovate Dashboard ðŸ¤–",
  "suppressNotifications": ["prIgnoreNotification"],
  "commitBodyTable": true,
  "rebaseWhen": "conflicted",
  "platformCommit": true,
  "assigneesFromCodeOwners": true,
  "forkProcessing": "enabled"
}
